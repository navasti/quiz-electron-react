{
	"name": "quiz-electron-react",
	"version": "0.0.1",
	"description": "Quiz application",
	"main": "./dist/main.bundle.js",
	"scripts": {
		"start": "electron ./dist/main.bundle.js",
		"dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
		"prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
		"test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
		"build:win": "electron-builder build --win --x64",
		"build:mac": "electron-builder build --mac --x64"
	},
	"build": {
		"appId": "com.navasti.quiz-electron-react",
		"productName": "Quiz Application",
		"directories": {
			"app": "./dist/",
			"output": "./out/"
		},
		"mac": {
			"target": "dmg"
		},
		"win": {
			"target": "nsis",
			"asar": true
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"buildVersion": "1"
	},
	"author": "Pawe≈Ç Szostak",
	"license": "MIT",
	"dependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@types/mocha": "^8.2.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router": "^5.1.8",
		"@types/react-router-dom": "^5.1.6",
		"copy-webpack-plugin": "^7.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.0.1",
		"electron": "^11.1.1",
		"electron-builder": "^22.9.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^4.5.0",
		"lodash": "^4.17.20",
		"mocha": "^8.2.1",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"source-map-loader": "^2.0.0",
		"spectron": "^13.0.0",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.12",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.1.3",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.3.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/navasti/quiz-electron-react"
	},
	"keywords": [
		"Electron",
		"React",
		"Typescript",
		"Webpack"
	],
	"bugs": {
		"url": "https://github.com/navasti/quiz-electron-react/issues"
	},
	"homepage": "https://github.com/navasti/quiz-electron-react#readme"
}
